
@{
    Layout = null;
}

<!DOCTYPE html>

<html class="no-js" lang="">
 <!--<![endif]-->/AdminLayout/assets/
<head>
    <title>AdminPortal</title>
    <link rel="apple-touch-icon" href="apple-icon.png">
    <link rel="shortcut icon" href="favicon.ico">
    <link rel="stylesheet" href="/AdminLayout/assets/css/normalize.css">
    <link rel="stylesheet" href="/AdminLayout/assets/css/bootstrap.min.css">
    <link rel="stylesheet" href="/AdminLayout/assets/css/font-awesome.min.css">
    <link rel="stylesheet" href="/AdminLayout/assets/css/themify-icons.css">
    <link rel="stylesheet" href="/AdminLayout/assets/css/flag-icon.min.css">
    <link rel="stylesheet" href="/AdminLayout/assets/css/cs-skin-elastic.css">
    <link rel="stylesheet" href="/AdminLayout/assets/scss/style.css">
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,600,700,800' rel='stylesheet' type='text/css'>
</head>
<body class="bg-dark">

    <div class="sufee-login d-flex align-content-center flex-wrap">
        <div class="container">
            <div class="login-content">
                <div class="login-logo">
                    <a href="/Admin/AdminLogin">
                        <h2>Travalia Admin</h2>
                    </a>
                </div>
                <div class="login-form">
                    <form  action="/Admin/AdminLogin" method="post">
                        <div class="form-group">
                            <label>Email address</label>
                            <input type="email" name="email" class="form-control" placeholder="Email">
                        </div>
                        <div class="form-group">
                            <label>Password</label>
                            <input type="password" name="password" class="form-control" placeholder="Password">
                        </div>
                        <div class="checkbox">
                            <label>
                                <input type="checkbox"> Remember Me
                            </label>
                            <label class="pull-right">
                                <a href="#">Forgotten Password?</a>
                            </label>
                        </div>
                        <input id="Submit1" type="submit" class="btn btn-success btn-flat m-b-30 m-t-30" value="Sign in" />
                        <div class="register-link m-t-15 text-center">
                            <a href="/User/HomePage">Back to Home</a>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>

    <script src="/AdminLayout/assets/js/vendor/jquery-2.1.4.min.js"></script>
    <script src="/AdminLayout/assets/js/popper.min.js"></script>
    <script src="/AdminLayout/assets/js/plugins.js"></script>
    <script src="/AdminLayout/assets/js/main.js"></script>

</body>
</html>
